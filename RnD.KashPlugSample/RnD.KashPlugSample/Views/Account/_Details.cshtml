<div id="detailsAccountPage">
    <div class="row-fluid">
        <div class="control-group">
            <label class="control-label" for="AccountName">
                Account Name:</label>
            <div class="controls">
                @Html.EditorFor(model => model.AccountName)
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div id="detailsCategoryButtonZone" class="pull-right">
            <button type="button" class="btn btn-warning" id="btnCancel" name="btnCancel" onclick=" $($(this).dialog('close'));">
                Cancel</button>
        </div>
    </div>
</div>
